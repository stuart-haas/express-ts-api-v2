{
  "name": "express-api",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
    "dev:build": "docker exec express_api npm run build && docker exec -it express_api chmod +x ./dist/cli/index.js",
    "dev:cli": "docker exec -it express_api dist/cli/index.js",
    "dev:test": "docker exec -it express_api npm test",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --fix --ext .ts",
    "test": "NODE_ENV=test jest",
    "precommit": "lint-staged",
    "db:migrate": "docker exec -it express_api npx sequelize-cli db:migrate",
    "db:migrate:undo": "docker exec -it express_api npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "docker exec -it express_api npx sequelize-cli db:migrate:undo:all",
    "db:seed": "docker exec -it express_api npx sequelize-cli db:seed",
    "db:seed:undo": "docker exec -it express_api npx sequelize-cli db:seed:undo",
    "db:seed:all": "docker exec -it express_api npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "docker exec -it express_api npx sequelize-cli db:seed:undo:all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run fix"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bullmq": "^1.86.5",
    "connect-session-sequelize": "^7.1.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "qs": "^6.10.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.12.3",
    "sequelize-typescript": "^2.1.3",
    "stack-utils": "^2.0.5",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@types/validator": "^13.7.4",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^4.3.8",
    "jest": "^27.4.3",
    "jest-junit": "^13.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.3.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
